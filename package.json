{
	"name": "rave-curl-domready",
	"version": "0.4.0",
	"description": "Adds curl's domReady AMD plugin to RaveJS.",
	"main": "rave.js",
	"rave": {
		"extension": "rave",
		"overrides": {
			"curl": {
				"directories": {
					"lib": "bower_components/curl/src/curl"
				}
			}
		}
	},
	"peerDependencies": {
		"curl": "~0.8",
		"rave-amd-plugins": "~0.4"
	},
	"homepage": "https://github.com/unscriptable/rave-curl-domready",
	"contributors": [
		"John Hann (http://unscriptable.com)",
		"Brian Cavalier (http://hovercraftstudios.com)"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:unscriptable/rave-curl-domready.git"
	},
	"keywords": [
		"rave",
		"rave-extension",
		"cujo",
		"cujojs",
		"amd",
		"plugin"
	],
	"author": "John Hann (http://unscriptable.com)",
	"license": "MIT"
}
